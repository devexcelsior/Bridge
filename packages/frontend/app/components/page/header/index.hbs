<header class="page-header" ...attributes>
  <NavigationNarrator @routeChangeValidator={{@routeChangeValidator}} />
  <LinkTo @route="index" class="doc-page-header__logo" aria-label="home page">
    logo here
  </LinkTo>
  <nav class="page-header__nav-menu" aria-label="primary navigation" id="primary-navigation">
    <ul role="list" aria-labelledby="primary-navigation">
      <li>
        <LinkTo @route="index">Home</LinkTo>
      </li>
      <li>
        <LinkTo @route="customers">Customers</LinkTo>
      </li>
      <li>
        <button type="button" {{on "click" this.toggleSidebar}}>Toggle Sidebar</button>
      </li>
    </ul>
  </nav>
</header>