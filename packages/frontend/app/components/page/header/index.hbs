
  <div class="hds-app-frame__header-inner theme-dark">
    <NavigationNarrator @routeChangeValidator={{@routeChangeValidator}} />
    <LinkTo @route="index" class="hds-app-frame__header-link" aria-label="home page">
      <img class="hds-app-frame__header-link-logo" src="/images/bridge-logo-white.png" alt="Bridge">
    </LinkTo>
    <nav class="hds-app-frame__header-nav-menu" aria-label="primary navigation" id="primary-navigation">
      <ul role="list" aria-labelledby="primary-navigation">
        <li>
          <LinkTo @route="index">Home</LinkTo>
        </li>
        <li>
          <LinkTo @route="customers">Customers</LinkTo>
        </li>
        <li>
          <button type="button" {{on "click" this.toggleSidebar}}>Toggle Sidebar</button>
        </li>
      </ul>
    </nav>
  </div>
